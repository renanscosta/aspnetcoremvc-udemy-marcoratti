@model Lanche
<div class="col col-sm-4 col-lg-2 col-md-2">
    <div class="img-thumbnail">
        <p><img src="@Url.Content(Model.ImagemUrl)" class="rounded" alt="" /></p>
    </div>
    <div class="caption">
        <h3 class="pull-righ">@Model.Preco.ToString("c")</h3>
        <h3>@Model.Nome</h3>
        <p>@Model.DescricaoCurta</p>
    </div>
</div>